{
	"name": "budget",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"format": "prettier --write .",
		"lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
